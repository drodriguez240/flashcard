{
  "name": "lazycard",
  "version": "1.0.0",
  "description": "",
  "main": "./build/main.js",
  "scripts": {
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "start": "cross-env NODE_ENV=development electron .",
    "server": "webpack-dev-server --mode development",
    "dev": "npm run build:dev && concurrently -s first -k \"npm run server\" \"npm run start\"",
    "pack": "npm run build:prod && electron-builder --dir",
    "dist": "npm run build:prod && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "com.example.electron",
    "files": [
      "build/**/*"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
