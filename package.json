{
  "name": "lazycard",
  "version": "1.0.1",
  "description": "A simple flashcard application where cards are formatted with Markdown.",
  "main": "./build/main.js",
  "scripts": {
    "dev": "webpack --mode development",
    "prod": "webpack --mode production",
    "start": "npm run dev && electron .",
    "pack": "npm run prod && electron-builder --dir",
    "dist": "npm run prod && electron-builder -p never",
    "test-with-xml-reporter": "mocha --require ts-node/register --reporter mocha-junit-reporter test/**/*.test.ts"
  },
  "keywords": [
    "flashcard",
    "spaced repetition",
    "markdown",
    "math",
    "katex",
    "electron"
  ],
  "author": "hikikones",
  "license": "MIT",
  "build": {
    "appId": "com.electron.lazycard",
    "productName": "Lazycard",
    "icon": "icon.png",
    "files": [
      "./build/**/*"
    ]
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/markdown-it": "0.0.9",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.45",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "electron": "^7.3.1",
    "electron-builder": "^21.2.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.2.3",
    "mocha-junit-reporter": "^1.23.3",
    "style-loader": "^1.2.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@iktakahiro/markdown-it-katex": "^3.1.0",
    "fuse.js": "^3.6.1",
    "katex": "^0.11.1",
    "lato-font": "^3.0.0",
    "markdown-it": "^10.0.0",
    "markdown-it-prism": "^2.0.5",
    "material-icons": "^0.3.1",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  }
}
