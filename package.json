{
  "name": "lazycard",
  "version": "1.0.0",
  "description": "A simple flashcard application where cards are formatted with Markdown.",
  "main": "./build/main.js",
  "scripts": {
    "dev": "webpack --mode development",
    "prod": "webpack --mode production",
    "start": "npm run dev && electron .",
    "pack": "npm run prod && electron-builder --dir",
    "dist": "npm run prod && electron-builder"
  },
  "keywords": ["flashcard", "spaced repetition", "markdown", "math", "katex", "electron"],
  "author": "hikikones",
  "license": "MIT",
  "build": {
    "appId": "com.electron.lazycard",
    "productName": "Lazycard",
    "icon": "icon.png",
    "files": [
      "./build/**/*"
    ]
  },
  "devDependencies": {
    "@types/markdown-it": "0.0.9",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.3.0",
    "electron": "^7.1.3",
    "electron-builder": "^21.2.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^1.0.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@iktakahiro/markdown-it-katex": "^3.1.0",
    "fuse.js": "^3.4.6",
    "katex": "^0.11.1",
    "lato-font": "^3.0.0",
    "markdown-it": "^10.0.0",
    "markdown-it-prism": "^2.0.3",
    "material-icons": "^0.3.1",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  }
}
